@model DesapegAutoWeb.Models.ConcessionariaViewModel

@{
    ViewData["Title"] = "Excluir Concessionária";
}

<h1>Excluir Concessionária</h1>
<hr />
<h3>Tem certeza que deseja excluir esta concessionária?</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Nome)</dt>
        <dd class="col-sm-10">@Model.Nome</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Cnpj)</dt>
        <dd class="col-sm-10">@Model.Cnpj</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Email)</dt>
        <dd class="col-sm-10">@Model.Email</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Telefone)</dt>
        <dd class="col-sm-10">@Model.Telefone</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Senha)</dt>
        <dd class="col-sm-10">@Model.Senha</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Endereco)</dt>
        <dd class="col-sm-10">@Model.Endereco</dd>
    </dl>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Excluir" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </form>
</div>
